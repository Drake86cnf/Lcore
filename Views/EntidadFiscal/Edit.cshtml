@model Lcore.Models.EntidadFiscal

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Edit</title>
</head>
<body>

<h4>EntidadFiscal</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="NumeroIdentificacionFiscal" class="control-label"></label>
                <input asp-for="NumeroIdentificacionFiscal" class="form-control" />
                <span asp-validation-for="NumeroIdentificacionFiscal" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TipoIdentificacionId" class="control-label"></label>
                <select asp-for="TipoIdentificacionId" class="form-control" asp-items="ViewBag.TipoIdentificacionId"></select>
                <span asp-validation-for="TipoIdentificacionId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SituacionTributariaId" class="control-label"></label>
                <select asp-for="SituacionTributariaId" class="form-control" asp-items="ViewBag.SituacionTributariaId"></select>
                <span asp-validation-for="SituacionTributariaId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Domicilio" class="control-label"></label>
                <input asp-for="Domicilio" class="form-control" />
                <span asp-validation-for="Domicilio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Comprobante" class="control-label"></label>
                <input asp-for="Comprobante" class="form-control" />
                <span asp-validation-for="Comprobante" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LocalidadId" class="control-label"></label>
                <select asp-for="LocalidadId" class="form-control" asp-items="ViewBag.LocalidadId"></select>
                <span asp-validation-for="LocalidadId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FechaCreado" class="control-label"></label>
                <input asp-for="FechaCreado" class="form-control" />
                <span asp-validation-for="FechaCreado" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FechaEditado" class="control-label"></label>
                <input asp-for="FechaEditado" class="form-control" />
                <span asp-validation-for="FechaEditado" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Nota" class="control-label"></label>
                <input asp-for="Nota" class="form-control" />
                <span asp-validation-for="Nota" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Activo" /> @Html.DisplayNameFor(model => model.Activo)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="RazonSocial" class="control-label"></label>
                <input asp-for="RazonSocial" class="form-control" />
                <span asp-validation-for="RazonSocial" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NombreFantasia" class="control-label"></label>
                <input asp-for="NombreFantasia" class="form-control" />
                <span asp-validation-for="NombreFantasia" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

</body>
</html>
